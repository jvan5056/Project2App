<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Subject selection page</title>
</head>
<body>
  <h1>Math Subjects</h1>
<hr>
<h2>
  <thead>
  <tr>
      <th>Select a math subject below.</th>
  </tr>
 </thead>
</h2>
<hr>
<ul>
  <% allSubjects.forEach((subject) => { %>
      <% include %> 
      <li id="subject-names">
         <a href=""><%= subject %></a><br>
      </li>
  <%})%>
  </ul>


  <a href="/classroom/resources"><button id="new-subject-button">Add Another Subject</button></a>
  <hr>
  <a href="/classroom">Back to Home page</a>
  <hr>
</body>
</html>



 
